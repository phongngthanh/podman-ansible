        statusCheckAllowInsecure: true
        statusCheckUrl: http://{{ caddyfile_host_address }}:8089/wolweb
        url: https://wol.{{ server_domain }}
      - title: Privatebin
        icon: hl-privatebin
        statusCheckAllowInsecure: true
