---
pageInfo:
  title: Zephyr Server
  description: Homelab server of Zephyr
  navLinks:
    - title: Home
      path: /
    - title: About
      path: /about
    - title: Source Code
      path: https://github.com/zephyros-dev/media-server
appConfig:
  statusCheck: true
  theme: Material
  iconSize: large
sections:
  - name: Personal
    items:
      - title: Nextcloud
        icon: https://nextcloud.{{ server_domain }}/core/img/favicon.ico
        url: https://nextcloud.{{ server_domain }}
      - title: Archivy
        icon: https://archivy.{{ server_domain }}/static/archivy.svg
        statusCheckAllowInsecure: true
        statusCheckUrl: http://archivy:5000
        url: https://archivy.{{ server_domain }}
      {# - title: Nocodb
        icon: https://nocodb.{{ server_domain }}/dashboard/favicon-32.png
        statusCheckAllowInsecure: true
        statusCheckUrl: http://nocodb:8080
        url: https://nocodb.{{ server_domain }} #}
      - title: Syncthing
        icon: https://syncthing.{{ server_domain }}/assets/img/favicon-default.png
        statusCheckAllowInsecure: true
        statusCheckUrl: http://{{ caddyfile_host_address }}:8384
        url: https://syncthing.{{ server_domain }}
      # - title: Gitea
      #   icon: favicon-local
      #   statusCheckAllowInsecure: true
      #   statusCheckUrl: http://gitea:3000
      #   url: https://gitea.{{ server_domain }}
  - name: Media Consumption
    items:
      - title: Calibreweb
        icon: https://calibrew.{{ server_domain }}/static/favicon.ico
        statusCheckAllowInsecure: true
        statusCheckUrl: http://calibrew:8083
        url: https://calibrew.{{ server_domain }}
      - title: Jellyfin
        icon: https://jellyfin.{{ server_domain }}/web/favicon.ico
        statusCheckAllowInsecure: true
        statusCheckUrl: http://jellyfin:8096
        url: https://jellyfin.{{ server_domain }}
      - title: Kavita
        icon: https://kavita.{{ server_domain }}/assets/images/logo.png
        statusCheckAllowInsecure: true
        statusCheckUrl: http://kavita:5000
        # Kavita has problem with firefox, dev refused to fix though, need to use workaround. Reference: https://github.com/Kareadita/Kavita/issues/1292
        url: https://kavita.{{ server_domain }}/login 
      - title: Komga
        icon: favicon-local
        statusCheckAllowInsecure: true
        statusCheckUrl: http://komga:8080
        url: https://komga.{{ server_domain }}
      - title: Navidrome
        icon: https://navidrome.{{ server_domain }}/app/android-chrome-192x192.png
        statusCheckAllowInsecure: true
        statusCheckUrl: http://navidrome:4533
        url: https://navidrome.{{ server_domain }}
  - name: Media management
    items:
      - title: Bazarr
        # icon: https://bazarr.{{ server_domain }}/static/favicon.ico
        icon: hl-bazarr
        statusCheckAllowInsecure: true
        statusCheckUrl: http://bazarr:6767
        url: https://bazarr.{{ server_domain }}
      - title: Lidarr
        icon: favicon-local
        statusCheckAllowInsecure: true
        statusCheckUrl: http://lidarr:8686
        url: https://lidarr.{{ server_domain }}
      - title: Prowlarr
        icon: favicon-local
        statusCheckAllowInsecure: true
        statusCheckUrl: http://prowlarr:9696
        url: https://prowlarr.{{ server_domain }}
      - title: Pymedusa
        icon: favicon-local
        statusCheckAllowInsecure: true
        statusCheckUrl: http://pymedusa:8081
        url: https://pymedusa.{{ server_domain }}
      - title: Radarr
        icon: favicon-local
        statusCheckAllowInsecure: true
        statusCheckUrl: http://radarr:7878
        url: https://radarr.{{ server_domain }}
      - title: Readarr
        icon: favicon-local
        statusCheckAllowInsecure: true
