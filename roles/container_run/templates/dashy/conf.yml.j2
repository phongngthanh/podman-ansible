        url: https://transmission.{{ server_domain }}
  - name: Monitoring
    items:
      - title: Librespeed
        icon: favicon-local
        statusCheckAllowInsecure: true
        statusCheckUrl: http://librespeed:80
        url: https://librespeed.{{ server_domain }}
      - title: Netdata
        icon: hl-netdata
        statusCheckAllowInsecure: true
        statusCheckUrl: http://{{ caddyfile_host_address }}:19999
        url: https://netdata.{{ server_domain }}
      - title: Scrutiny
        icon: https://scrutiny.{{ server_domain }}/web/assets/images/logo/scrutiny-logo-white.svg
        statusCheckAllowInsecure: true
        statusCheckUrl: http://{{ caddyfile_host_address }}:18080
        url: https://scrutiny.{{ server_domain }}
  - name: Misc
